{
   "name": "Send To Kiwi",
   "version": "2.0",
   "permissions": [
     "activeTab",
     "https://*/",
     "http://*/"
   ],
   "background": {
     "scripts": ["background.js"],
     "persistent": false
    },
    "browser_action": {
      "default_title": "Send to Kiwi",
      "default_icon": "kiwi.png"
    },
    "icons": { 
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png" 
      },
    "manifest_version": 2
}